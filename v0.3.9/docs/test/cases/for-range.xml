<?xml version="1.0" encoding="UTF-8"?>
<test xmlns:s="vs.templ">
    <data>
    </data>

    <template>
        <root>
            <s:for-range tag="i" from="5" to="10" step="2">
                <i>head</i>
                <s:for-range tag="j" from="{i}" to="10" step="2">
                    <item>(<s:value src="{i}" />;<s:value src="{j}" />)</item>
                </s:for-range>
            </s:for-range>
        </root>
    </template>

    <expects>
        <root>
            <i>head</i>
            <item>(5;5)</item>
            <item>(5;7)</item>
            <item>(5;9)</item>
            <i>head</i>
            <item>(7;7)</item>
            <item>(7;9)</item>
            <i>head</i>
            <item>(9;9)</item>
        </root>
    </expects>
</test>